{% extends 'base.html.twig' %}

{% block title %}Article Franchise{% endblock %}

{% block body %}
    <h1>Article</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ article.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ article.titre }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ article.contenu }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('adminFranchise_edit', {'id': article.id}) }}">edit</a>

    {{ include('franchise/_delete_form.html.twig') }}

    <a href="{{ path('adminFranchise_index') }}">back to list</a>

{% endblock %}
